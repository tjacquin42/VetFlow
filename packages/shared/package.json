{
  "name": "@vetflow/shared",
  "version": "0.1.0",
  "description": "Shared code for VetFlow (types, formulas, validators)",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./types/*": "./src/types/*",
    "./lib/*": "./src/lib/*"
  },
  "scripts": {
    "build": "tsc --build",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
